<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<typeAliases>
		<typeAlias type="com.ptsd.mvc.notice.NoticeDto" alias="noticeDto" />
		<typeAlias type="com.ptsd.mvc.user.UserDto" alias="userDto" />
		<typeAlias type="com.ptsd.mvc.product.ProductDto" alias="productDto" />
		<typeAlias type="com.ptsd.mvc.streaming.StreamingDto" alias="streamingDto"/>
	</typeAliases>

	<mappers>
		<mapper resource="/mybatis/notice-mapper.xml" />
		<mapper resource="/mybatis/user-mapper.xml" />
		<mapper resource="/mybatis/product-mapper.xml"/>
		<mapper resource="/mybatis/streaming-mapper.xml"/>
	</mappers>

</configuration>